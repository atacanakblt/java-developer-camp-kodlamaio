package classesWithAttributes;

public class Product {

	// attribute
	private int id;
	private String name;
	private String description;
	private double price;
	private int stockAmount;
	private String renk;
	private String kod;

	
	public int getId() {
		return id;
	}
	
	// this i√ßinde bulunulan class
	public void setId(int id) {
		this.id=id;
	}
	
}
